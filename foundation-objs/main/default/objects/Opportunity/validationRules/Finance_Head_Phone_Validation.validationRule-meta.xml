<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Finance_Head_Phone_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISBLANK( Finance_Head_Contact_Number__c )),
NOT(REGEX( Finance_Head_Contact_Number__c, &quot;[0-9]{10}&quot;))
)</errorConditionFormula>
    <errorMessage>Enter Valid Finance Head Contact Number</errorMessage>
</ValidationRule>
