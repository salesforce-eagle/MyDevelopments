<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MS_Start_date_end_date</fullName>
    <active>false</active>
    <errorConditionFormula>( MS_Start_Date__c &gt;  MS_End_Date__c )|| (  SOW_Start_Date__c &gt; SOW_End_Date__c )</errorConditionFormula>
    <errorDisplayField>MS_Start_Date__c</errorDisplayField>
    <errorMessage>MS/SOW End Date must be greater than MS/SOW Start Date</errorMessage>
</ValidationRule>
