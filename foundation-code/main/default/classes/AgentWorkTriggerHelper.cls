public class AgentWorkTriggerHelper {
    
    public static void updateCaseStatus(List<AgentWork> listOfNewList){
        system.debug('****Inside  updateCaseStatus ');
        Set<String> caseIds  = new Set<String>();
        for (AgentWork aw : listOfNewList) {
            String workitemId = aw.WorkItemId;
            System.debug('aw.Status '+aw.Status);
            if (workitemId.left(3) == '500') {    
                caseIds.add(workitemId);     
            }    
        }
        system.debug('****caseIds '+caseIds);
        List<Case> caseTobeUpdateList = new List<Case>();
        if(caseIds.size()>0){
            list<case> listOfCases=[select id,Status,Sub_Status__c,OwnerId from case where id in:caseIds];
            
            for(case cs : listOfCases){                
                if(cs.Status!='Pending' && cs.Status != 'Open'  && (cs.Sub_Status__c !='With customer for solution verification' 
                                                                                              || cs.Sub_Status__c !='With customer for problem clarification')){
                    system.debug('AgentWorkTriggerHelper reached here--->'+cs);
                    Case caseInstance = new Case(); 
                    caseInstance.id = cs.id;
                    caseInstance.Status = 'Open';
                    caseTobeUpdateList.add(caseInstance);
                }
            }
        }
        
        system.debug('****caseTobeUpdateList-->'+caseTobeUpdateList);
        
        if(caseTobeUpdateList.size()>0){
            system.debug('Inside Update caseTobeUpdateList '+caseTobeUpdateList);
            update caseTobeUpdateList;
        }
    }
    
